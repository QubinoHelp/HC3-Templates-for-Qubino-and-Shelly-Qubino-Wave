{
    "description": "Qubino Flush Dimmer",
    "parameters": [
        {
            "parameterNumber": 1,
            "label": {
                "en": "In-wall Switch Type for Load (Q1) to control I1",
                "pl": ""
            },
            "description": {
                "en": "With this parameter, you can select between momentary and on/off toggle switch types",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 0,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "momentary switch",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 1,
                        "description": {
                            "en": "on/off toggle switch",
                            "pl": ""
                        }
                    }
                ]
            }
        },
        {
            "parameterNumber": 2,
            "label": {
                "en": "In-wall Switch Type for Load 1 (Q1) to control I2",
                "pl": ""
            },
            "description": {
                "en": "With this parameter, you can select between momentary and on/off toggle switch types",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 0,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "momentary switch",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 1,
                        "description": {
                            "en": "on/off toggle switch",
                            "pl": ""
                        }
                    }
                ]
            }
        }, 
        {
            "parameterNumber": 3,
            "label": {
                "en": "Input 2 contact type",
                "pl": ""
            },
            "description": {
                "en": "Select between normally open (NO) or normally closed (NC) input type.",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 0,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "NO (normally open) input type",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 1,
                        "description": {
                            "en": "NC (normally close) input type",
                            "pl": ""
                        }
                    }
                ]
            }
        }, 
        {
            "parameterNumber": 4,
            "label": {
                "en": "Input 3 contact type",
                "pl": ""
            },
            "description": {
                "en": "Select between normally open (NO) or normally closed (NC) input type.",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 0,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "NO (normally open) input type",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 1,
                        "description": {
                            "en": "NC (normally close) input type",
                            "pl": ""
                        }
                    }
                ]
            }
        },
        {
            "parameterNumber": 10,
            "label": {
                "en": "Activate / deactivate ALL ON / ALL OFF Functionality",
                "pl": ""
            },
            "description": {
                "en": "Flush Dimmer device responds to the commands ALL ON / ALL OFF that may be sent by the primary or secondary gateway (hub) within the Z-Wave network.",
                "pl": ""
            },
            "size": 2,
            "defaultValue": 255,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 255,
                        "description": {
                            "en": "ALL ON active, ALL OFF active",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "ALL ON not active, ALL OFF not active",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 1,
                        "description": {
                            "en": "ALL ON not active, ALL OFF active",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 2,
                        "description": {
                            "en": "ALL ON active, ALL OFF not active",
                            "pl": ""
                        }
                    }
                ]
            }
        },
        {
            "parameterNumber": 11,
            "label": {
                "en": "Turn Load Off Automatically with Timer",
                "pl": ""
            },
            "description": {
                "en": "If Load 1 is ON, you can schedule it to turn OFF automatically after a period of time defined in this parameter. The timer is reset to zero each time the device receives an ON command.",
                "pl": ""
            },
            "size": 2,
            "defaultValue": 0,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "Auto OFF Disabled",
                            "pl": ""
                        }
                    },
                    {
                        "type": "range",
                        "min": 1,
                        "max": 32535,
                        "step": 1,
                        "scale": 1.0,
                        "unit": "s"
                    }
                ]
            }
        },
        {
            "parameterNumber": 12,
            "label": {
                "en": "Turn Load On Automatically with Timer",
                "pl": ""
            },
            "description": {
                "en": "If Load is OFF, you can schedule it to turn ON automatically after a period of time defined in this parameter. The timer is reset to zero each time the device receives an OFF command.",
                "pl": ""
            },
            "size": 2,
            "defaultValue": 0,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "Auto OFF Disabled",
                            "pl": ""
                        }
                    },
                    {
                        "type": "range",
                        "min": 1,
                        "max": 32535,
                        "step": 1,
                        "scale": 1.0,
                        "unit": "s"
                    }
                ]
            }
        },
        {
            "parameterNumber": 20,
            "label": {
                "en": "Enable/Disable the 3-way switch/additional switch",
                "pl": ""
            },
            "description": {
                "en": "If the the 3-way switch option is set, dimming can be controlled by a push-button or a switch, connected to I1 and I2.",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 0,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "single push-button (connected to I1)",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 1,
                        "description": {
                            "en": "3-way switch (connected to I1 and I2)",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 2,
                        "description": {
                            "en": "Additional switch (connected to I2)",
                            "pl": ""
                        }
                    }
                ]
            }
        },
        {
            "parameterNumber": 21,
            "label": {
                "en": "Enable/Disable the Double click function",
                "pl": ""
            },
            "description": {
                "en": "If the Double click function is enabled, a fast double click on the momentary switch sets the dimming level to the maximum dimming value",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 0,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "double click disabled",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 1,
                        "description": {
                            "en": "double click enabled",
                            "pl": ""
                        }
                    }
                ]
            }
        },
        {
            "parameterNumber": 30,
            "label": {
                "en": "Restore on/off status for load after power failure",
                "pl": ""
            },
            "description": {
                "en": "This parameter determines if on/off status is saved and restored for the load Q after power failure",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 0,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "Device saves last on/off status and restores it after a power failure.",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 1,
                        "description": {
                            "en": "Device does not save on/off status and does not restore it after a power failure, it remains off",
                            "pl": ""
                        }
                    }
                ]
            }
        },
        {
            "parameterNumber": 40,
            "label": {
                "en": "Watt Power Consumption Reporting Threshold for Load",
                "pl": ""
            },
            "description": {
                "en": "Choose by how much the power consumption needs to increase or decrease to be reported. Values correspond to percentages.",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 10,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "Power consumption reporting disabled",
                            "pl": ""
                        }
                    },
                    {
                        "type": "range",
                        "min": 1,
                        "max": 100,
                        "step": 1,
                        "scale": 1.0,
                        "unit": "%"
                    }
                ]
            }
        },
        {
            "parameterNumber": 42,
            "label": {
                "en": "Watt Power Consumption Reporting Time Threshold for Q Load",
                "pl": ""
            },
            "description": {
                "en": "The set value refers to the time interval with which the power consumption in Watts is reported (0 – 32767 seconds). If 0 is entered, energy consumption reports will not be sent to the gateway (hub)",
                "pl": ""
            },
            "size": 2,
            "defaultValue": 0,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "Power consumption reporting disabled",
                            "pl": ""
                        }
                    },
                    {
                        "type": "range",
                        "min": 30,
                        "max": 32767,
                        "step": 1,
                        "scale": 1.0,
                        "unit": "s"
                    }
                ]
            }
        },
        {
            "parameterNumber": 60,
            "label": {
                "en": "Minimum dimming value",
                "pl": ""
            },
            "description": {
                "en": "The value set in this parameter determines the minimum dimming value, the lowest value which can be set on the device",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 1,
            "format": {
                "type": "range",
                "min": 1,
                "max": 98,
                "step": 1,
                "scale": 1.0,
                "unit": "%"
            }
        },
        {
            "parameterNumber": 61,
            "label": {
                "en": "Maximum dimming value",
                "pl": ""
            },
            "description": {
                "en": "The value set in this parameter determines the maximum dimming value, the highest value which can be set on the device",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 99,
            "format": {
                "type": "range",
                "min": 2,
                "max": 99,
                "step": 1,
                "scale": 1.0,
                "unit": "%"
            }
        },
        {
            "parameterNumber": 65,
            "label": {
                "en": "Dimming time when key pressed (soft on/off)",
                "pl": ""
            },
            "description": {
                "en": "Choose the time during which the device will move between the min. and max. dimming values by a short press of the momentary switch connected to terminal I",
                "pl": ""
            },
            "size": 2,
            "defaultValue": 100,
            "format": {
                "type": "range",
                "min": 50,
                "max": 255,
                "step": 1,
                "scale": 1.0,
                "unit": "ms"
            }
        },
        {
            "parameterNumber": 66,
            "label": {
                "en": "Dimming time when key hold",
                "pl": ""
            },
            "description": {
                "en": "Choose the time during which the Dimmer will move between the min. and max. dimming values during a continuous press of the momentary switch connected to terminal I",
                "pl": ""
            },
            "size": 2,
            "defaultValue": 3,
            "format": {
                "type": "range",
                "min": 1,
                "max": 255,
                "step": 1,
                "scale": 1.0,
                "unit": "s"
            }
        },
        {
            "parameterNumber": 67,
            "label": {
                "en": "Ignore start level",
                "pl": ""
            },
            "description": {
                "en": "Choose whether the device should use (or disregard) the start dimming level value. This parameter is used with association group 3",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 0,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "use the start level value",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 1,
                        "description": {
                            "en": " ignore the start level value",
                            "pl": ""
                        }
                    }
                ]
            }
        },
        {
            "parameterNumber": 68,
            "label": {
                "en": "Dimming duration for associations",
                "pl": ""
            },
            "description": {
                "en": "Choose the time during which the device will transition from the current value to the new target value. This parameter applies to the association group 3.",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 0,
            "format": {
                "type": "range",
                "min": 0,
                "max": 127,
                "step": 1,
                "scale": 1.0,
                "unit": "s"
            }
        },
        {
            "parameterNumber": 100,
            "label": {
                "en": "Enable / Disable Endpoint I2 or select the Notification Type and the Notification Event",
                "pl": ""
            },
            "description": {
                "en": "Choose whether the Endpoint I2 is disabled or enabled. By enabling this endpoint, the user also selects a Notification Type and a Notification Event for which notification reports will be sent",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 0,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": " Endpoint, I2 disabled",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 1,
                        "description": {
                            "en": "Home Security; Motion Detection",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 2,
                        "description": {
                            "en": "CO; Carbon Monoxide detected",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 3,
                        "description": {
                            "en": "CO2; Carbon Dioxide detected",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 4,
                        "description": {
                            "en": "Water Alarm; Water Leak detected",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 5,
                        "description": {
                            "en": "Heat Alarm; Overheat detected",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 6,
                        "description": {
                            "en": "Smoke Alarm; Smoke detected",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 9,
                        "description": {
                            "en": "Sensor binary - generic",
                            "pl": ""
                        }
                    }
                ]
            }
        },
        {
            "parameterNumber": 101,
            "label": {
                "en": "Enable / Disable Endpoint I3 or select the Notification Type and the Notification Event",
                "pl": ""
            },
            "description": {
                "en": "Choose whether the Endpoint I3 is disabled or enabled. By enabling this endpoint, the user also selects a Notification Type and a Notification Event for which notification reports will be sent",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 0,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": " Endpoint, I2 disabled",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 1,
                        "description": {
                            "en": "Home Security; Motion Detection",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 2,
                        "description": {
                            "en": "CO; Carbon Monoxide detected",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 3,
                        "description": {
                            "en": "CO2; Carbon Dioxide detected",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 4,
                        "description": {
                            "en": "Water Alarm; Water Leak detected",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 5,
                        "description": {
                            "en": "Heat Alarm; Overheat detected",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 6,
                        "description": {
                            "en": "Smoke Alarm; Smoke detected",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 9,
                        "description": {
                            "en": "Sensor binary - generic",
                            "pl": ""
                        }
                    }
                ]
            }
        },
        {
            "parameterNumber": 110,
            "label": {
                "en": "Temperature Sensor Offset Settings",
                "pl": ""
            },
            "description": {
                "en": "Set value is added to or subtracted from the actually measured value to adjust the temperature report sent by an external sensor (sold separately).",
                "pl": ""
            },
            "size": 2,
            "defaultValue": 32536,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 32536,
                        "description": {
                            "en": "Offset is 0°C",
                            "pl": ""
                        }
                    },
                    {
                        "type": "range",
                        "min": 1,
                        "max": 100,
                        "step": 1,
                        "scale": 0.1
                    },
                    {
                        "type": "range",
                        "min": 1001,
                        "max": 1100,
                        "step": 1,
                        "scale": 0.1
                    }
                ]
            }
        },
        {
            "parameterNumber": 120,
            "label": {
                "en": " Temperature Sensor Reporting Threshold",
                "pl": ""
            },
            "description": {
                "en": "If an external digital temperature sensor (sold separately) is connected to the device, it reports temperature readings based on the threshold defined in this parameter. ",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 5,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "Reporting disabled",
                            "pl": ""
                        }
                    },
                    {
                        "type": "range",
                        "min": 1,
                        "max": 127,
                        "step": 1,
                        "scale": 0.1
                    }
                ]
            }
        },
        {
            "parameterNumber": 249,
            "label": {
                "en": "Enable/Disable Reporting on Set command",
                "pl": ""
            },
            "description": {
                "en": "Choose whether reports are sent to the gateway (hub) after a set command is received by the device",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 1,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "Disable reporting",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 1,
                        "description": {
                            "en": "Enable reporting",
                            "pl": ""
                        }
                    }
                ]
            }
        }
    ]
}